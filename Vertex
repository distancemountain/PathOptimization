package com.huawei.it.optimization.common.TrafficOptimizationUtils;

import java.io.Serializable;
import java.util.List;

/**
 * @version :1.0
 * @author: Glory.Y
 * @description: Vertex
 * @since: 2019-3-14
 */
public class Vertex implements Serializable {
    /** . */
    private long id;
    /** . */
    private long cityId;
    /** . */
    private String name;
    /** . */
    private String level;
    /** . */
    private List<Edge> neighbourEdge;
    /** . */
    private List<Vertex> neighbourVertex;
    /** . */
    private int distance;
    /** . */
    private List<Edge> edgePath;
    /** . */
    private List<Vertex> vertexPath;

    /**
     *
     * @return l
     */
    public final long getId() {
        return id;
    }

    /**
     *
     * @param id1 i
     */
    public final void setId(final long id1) {
        this.id = id1;
    }

    /**
     *
     * @return l
     */
    public final long getCityId() {
        return cityId;
    }

    /**
     *
     * @param cityId1 c
     */
    public final void setCityId(final long cityId1) {
        this.cityId = cityId1;
    }

    /**
     *
     * @return s
     */
    public final String getName() {
        return name;
    }

    /**
     *
     * @param name1 s
     */
    public final void setName(final String name1) {
        this.name = name1;
    }

    /**
     *
     * @return s
     */
    public final String getLevel() {
        return level;
    }

    /**
     *
     * @param level1 l
     */
    public final void setLevel(final String level1) {
        this.level = level1;
    }

    /**
     *
     * @return l
     */
    public final List<Edge> getNeighbourEdge() {
        return neighbourEdge;
    }

    /**
     *
     * @param neighbourEdge1 n
     */
    public final void setNeighbourEdge(final List<Edge> neighbourEdge1) {
        this.neighbourEdge = neighbourEdge1;
    }

    /**
     *
     * @return l
     */
    public final List<Vertex> getNeighbourVertex() {
        return neighbourVertex;
    }

    /**
     *
     * @param neighbourVertex1 n
     */
    public final void setNeighbourVertex(final List<Vertex> neighbourVertex1) {
        this.neighbourVertex = neighbourVertex1;
    }

    /**
     *
     * @return i
     */
    public final int getDistance() {
        return distance;
    }

    /**
     *
     * @param distance1 v
     */
    public final void setDistance(final int distance1) {
        this.distance = distance1;
    }

    /**
     *
     * @return list
     */
    public final List<Edge> getEdgePath() {
        return edgePath;
    }

    /**
     *
     * @param edgePath1 v
     */
    public final void setEdgePath(final List<Edge> edgePath1) {
        this.edgePath = edgePath1;
    }

    /**
     *
     * @return list
     */
    public final List<Vertex> getVertexPath() {
        return vertexPath;
    }

    /**
     *
     * @param vertexPath1 f
     */
    public final void setVertexPath(final List<Vertex> vertexPath1) {
        this.vertexPath = vertexPath1;
    }
}
